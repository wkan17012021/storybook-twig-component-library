{{ attach_library('mytheme/quote-carousel') }}

{% embed '@molecules/section/section.twig' with {
  indent_bg: true,
  indent_bg_side: 'right',
  indent_bg_2xl: true,
  classes: [
    'quote-carousel',
    "md:u-bg-[url(data:image/svg+xml;base64,)]",
    'u-bg-no-repeat',
    'u-bg-cover',
  ],
  container_classes: ['swiper'],
  section_content: section_content,
} %}
  {% block section_content %}
    <div class="swiper-wrapper u-relative u-z-10">
      {{ section_content }}
    </div>
  {% endblock %}
  {% block section_footer %}
    <div class="u-text-center md:u-text-left u-p-4 md:u-p-7 lg:u-p-9">
      <div class="swiper-paging"></div>
    </div>
  {% endblock %}
{% endembed %}
